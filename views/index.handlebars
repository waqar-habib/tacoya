<div class="row">
    <div class="col"></div>
    <div class="col">
        <div class="logo">
                <img src="/assets/img/tacoya.jpg" width="300px" alt="Logo">
        </div>
    </div>
    <div class="col"></div>
</div>

<div class="row">
    <div class="col-md-12 text-center">
        <p class="text">Choose or Order a Taco to eat and Track What you Eat!</p>
            <form class="form">
                <div class="form-group">
                    <input type="text" placeholder="Taco name" id="inputTaco" name="name">
                </div>
                <button id="submit-btn" type="submit">Order Taco!</button>
            </form>
        </div>
    </div>
    <div class="row">
        <div class="col-md-5 eat">
            <h2><strong>Available Tacos<strong></h2>
            <ul>
                {{#each tacos}} {{#unless picked_up}} {{> tacos/taco-block pick_up=true}} {{/unless}} {{/each}}
            </ul>
        </div>
        <div class="col-md-2"></div>
        <div class="col-md-8 eaten">
            <h2><strong>Already Eaten!<strong></h2>
            <ul>
                {{#each tacos}} {{#if picked_up}}
                <div class="tacomenu">
                    <span class="label">#{{this.id}}</span>
                    <span class="label">{{this.taco_name}}</span>
                </div>
                {{/if}} {{/each}}
            </ul>
        </div>
        <div class="col-md-1"></div>
    </div>